{
  "Variables": [
    "Request",
    "RequestLine",
    "Method",
    "URI",
    "URITail",
    "Version",
    "Headers",
    "Header",
    "HeaderName",
    "HeaderValue",
    "ValueTail",
    "ValueToken"
  ],

  "Terminals": [
    "METHOD_GET",
    "METHOD_POST",
    "METHOD_HEAD",
    "SLASH",
    "DOT",
    "IDENT",
    "SP",
    "COLON",
    "HTTP_VERSION_1_0",
    "CRLF"
  ],

  "Start": "Request",

  "Productions": [
    { "head": "Request", "body": ["RequestLine", "Headers", "CRLF"] },

    {
      "head": "RequestLine",
      "body": ["Method", "SP", "URI", "SP", "Version", "CRLF"]
    },

    { "head": "Method", "body": ["METHOD_GET"] },
    { "head": "Method", "body": ["METHOD_POST"] },
    { "head": "Method", "body": ["METHOD_HEAD"] },

    { "head": "URI", "body": ["SLASH", "IDENT", "URITail"] },

    { "head": "URITail", "body": ["DOT", "IDENT", "URITail"] },
    { "head": "URITail", "body": ["SLASH", "IDENT", "URITail"] },
    { "head": "URITail", "body": [] },

    { "head": "Version", "body": ["HTTP_VERSION_1_0"] },

    { "head": "Headers", "body": ["Header", "Headers"] },
    { "head": "Headers", "body": [] },

    {
      "head": "Header",
      "body": ["HeaderName", "COLON", "SP", "HeaderValue", "CRLF"]
    },

    { "head": "HeaderName", "body": ["IDENT"] },

    { "head": "HeaderValue", "body": ["ValueToken", "ValueTail"] },

    { "head": "ValueTail", "body": ["SP", "ValueToken", "ValueTail"] },
    { "head": "ValueTail", "body": ["SLASH", "ValueToken", "ValueTail"] },
    { "head": "ValueTail", "body": [] },

    { "head": "ValueToken", "body": ["IDENT"] }
  ]
}
